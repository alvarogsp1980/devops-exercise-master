{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "subscriptionId": {
            "value": "00000000-0000-0000-0000-000000000000"
        },
        "resourceGroupName": {
            "value": "yourResourceGroup"
        },
        "name": {
            "value": "yourAppName"
        },
        "location": {
            "value": "YourRegion"
